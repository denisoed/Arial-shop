    +e.DIV.container
      +b.DIV.partners-title
        +sectionTitle("Our partners")
      +b.DIV.partners-carousel.owl-carousel
        +e.IMG.image(src="img/partners/1.png")
        +e.IMG(src="img/partners/2.png")
        +e.IMG(src="img/partners/3.png")
        +e.IMG(src="img/partners/4.png")
        +e.IMG(src="img/partners/5.png")
        +e.IMG(src="img/partners/6.png")
        +e.IMG(src="img/partners/7.png")
  
